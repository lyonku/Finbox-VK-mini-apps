ms.length; i++) {\n          for (let j = 0; j < members.length; j++) {\n            if (userGroup.response.items[i].id == members[j]) {\n              adminsGroups.push(userGroup.response.items[i]);\n            }\n          }\n        }\n        if (adminsGroups.length >= 1) {\n          setTrueUser(true);\n          setAdminsGroup(adminsGroups);\n        } else {\n          setTrueUser(false);\n        }\n      }\n      fetchUser();\n    }\n  }, [activeOrganization, checkUser]);\n\n  const go = (e) => {\n    if (e.target.id) {\n      setActiveOrganization(e.target.id);\n    }\n    console.log(\"page\" + e.currentTarget.dataset.page);\n\n    if (e.currentTarget.dataset.page === \"comments\") {\n      setGroup(null);\n    }\n    setActivePanel(e.currentTarget.dataset.to);\n  };\n\n  return (\n    <ConfigProvider scheme={scheme}>\n      <AdaptivityProvider>\n        <AppRoot>\n          <SplitLayout>\n            <SplitCol>\n              <View activePanel={activePanel}>\n                <Home\n                  id=\"home\"\n                  go={go}\n                  onChange={handleSumChange}\n                  rngValue={rngValue}\n                />\n                <Offers\n                  id=\"offers\"\n                  go={go}\n                  rngValue={rngValue}\n                  group={group}\n                  rateResult={rateResult}\n                />\n                <Comments\n                  id=\"comments\"\n                  go={go}\n                  user={user}\n                  group={group}\n                  rateResult={rateResult}\n                />\n                <Membership\n                  id=\"membership\"\n                  go={go}\n                  setCheckUser={setCheckUser}\n                  trueUser={trueUser}\n                  adminsGroup={adminsGroup}\n                />\n              </View>\n            </SplitCol>\n          </SplitLayout>\n        </AppRoot>\n      </AdaptivityProvider>\n    </ConfigProvider>\n 